<h1>Projects List</h1>

<table class='table' id='project_table'>
  <thead>
  <tr>
    <th>File Name</th>
    <th>Last Updated</th>
  </tr>
  </thead>
  <tbody>
    <tr>
      <% @projects.each do |project| %>
        <td><%= link_to project.document_file_name, project.document_file_name %></td>
        <td><%= project.updated_at %></td>
        <td><%= link_to 'Edit', "#" %></td>
        <td><%= link_to 'Destroy', "#" %></td>
          <%=render partial: 'table_row', locals: {project: project} %>
      <% end %>
    </tr>
  </tbody>

 </table>
